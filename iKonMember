import java.util.Scanner;

public class iKonMember {
	
	public static void main (String args[]) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter iKon member:");
		String iKonMember = sc.nextLine();
		EvaluateMember(iKonMember);
		sc.close();
	}
	
	public static void EvaluateMember(String iKonMember) {
		
		if (iKonMember != null) {
			if (!iKonMember.isEmpty()) {
				if (iKonMember. equals("Bobby")||
						iKonMember.equalsIgnoreCase("Jiwon")||
						iKonMember.equalsIgnoreCase("B.I")||
						iKonMember.equalsIgnoreCase("Hanbin")||
						iKonMember.equalsIgnoreCase("Jinhwan")||
						iKonMember.equalsIgnoreCase("Jonhoe")||
						iKonMember.equalsIgnoreCase("Donghyuk")||
						iKonMember.equalsIgnoreCase("Yunhyeong")) {
					System.out.println(iKonMember + " is an original member.");
				}
				
				else if (iKonMember.equalsIgnoreCase("Chanwoo")) {
						System.out.println(iKonMember + " is not an original member.");
					}
			
				else if (iKonMember.equalsIgnoreCase("Jinhyung")||
						iKonMember.equalsIgnoreCase("Hongseok")) {
						System.out.println(iKonMember + " did not make it into iKon.");
					}
			
					else {
							System.out.println (iKonMember + " is not a valid entry.");
						}
				}
				else {
						System.out.println ("iKon Member is an empty string.");
					}
			}
			else {
					System.out.println ("You have given me a null string.");
				}
			}
		}
