
public class LCRoster {
	public static void main(String[] args) {
		
		
		LCRoster LC3 = new LCRoster("piSquad", 24);
		System.out.println("LC3's name is " + LC3.getName());
		System.out.println("LC3 has " + LC3.getID() + " people");
		
		studentArray (LC3.getListedStudents());
		}
	
	
		public LCRoster(String name,int ID) {
			setName(name);
			setID(ID);
			}
	String LCName = "piSquad";
	int LCID = 24;
	String[] listedStudents = {"Brandon, Kinsler, Dionne"};
	
	 public String getName() {
		return LCName;
	}
	 
	private void setName (String name) {
		LCName = name;
	}
	
	public int getID() {
		return LCID;
	}
	
	private void setID (int ID) {
		LCID = ID;
	}
	
	public String[] getListedStudents () {
		return listedStudents;
	}
	public static String[] studentArray (String [] listedStudents) {
		if (listedStudents != null) {
			for (String y: listedStudents) {
				System.out.println(y + " are students of LC3");
			}
		}return listedStudents;
	}
}

